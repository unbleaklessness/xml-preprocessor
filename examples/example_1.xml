<mujoco>
	<variable name="x_pos" value="0"/>
	<variable name="y_pos" value="1"/>
	<variable name="z_pos" value="2"/>
  <variable name="c_radius" value="sqrt(${z_pos})"/>
  <variable name="c_size" value="${y_pos} + 1"/>
	<worldbody>
		<body pos="${x_pos} ${y_pos} ${z_pos}">
			<geom type="cylinder" size="${c_radius} ${c_size}"/>
		</body>
	</worldbody>
</mujoco>